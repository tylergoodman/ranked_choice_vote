{
  "name": "ranked_choice_vote",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "^18.15.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "run-p --print-label dev-be dev-fe",
    "dev-fe": "vite",
    "dev-be": "tsx watch src/server/api.ts --tsconfig=tsconfig.server.json --clear-screen=false",
    "start": "run-p --print-label start-be start-fe",
    "start-be": "tsx src/server/api.ts",
    "start-fe": "vite preview",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "npm run lint -- --fix",
    "test": "playwright test",
    "test-watch": "PWTEST_WATCH=1 playwright test"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@tanstack/react-query": "^4.29.14",
    "@trpc/client": "^10.30.0",
    "@trpc/next": "^10.30.0",
    "@trpc/react-query": "^10.30.0",
    "@trpc/server": "^10.30.0",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "superjson": "^1.12.3",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@tanstack/react-query-devtools": "^4.29.14",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "start-server-and-test": "^2.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "private": true,
  "license": "Public Domain"
}